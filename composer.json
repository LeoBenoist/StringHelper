{
  "name": "leobenoist/string-helper",
  "type": "library",
  "description": "PHP StringHelper is a small library that can perform string manipulation.",
  "keywords": ["php", "strings", "words", "manipulation", "uppercase", "lowercase", "camelcase", "snakecase", "prefix", "suffix", "startwith", "endwith"],
  "license": "MIT",
  "authors": [
    {"name": "Leo Benoist", "email": "jobs.leo@benoi.st"}
  ],
  "require": {
    "php": "^7.1||^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": { "LeoBenoist\\StringHelper\\": "lib" }
  },
  "autoload-dev": {
    "psr-4": { "LeoBenoist\\Tests\\StringHelper\\": "tests" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
